    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String Username,password;
        
       Username = User.getText();
       password = Pass.getText();
       String query = "SELECT * FROM admins WHERE User ='" +Username +"'AND Pass='" +password +"'";
       
        try {
            PS = con.prepareStatement(query);
            ResultSet RS = PS.executeQuery(); 
            if(!RS.next()){
                JOptionPane.showMessageDialog(null, "ERROR!");
             if("".equals(User.getText())){
            JOptionPane.showMessageDialog(new JFrame(),"You haven't entered your username");
            
             }if("".equals(Pass.getText())){
            JOptionPane.showMessageDialog(new JFrame(),"You haven't entered your Password");
            
        }
            }else{
             JOptionPane.showMessageDialog(null, "You have Logged in Successfully!");
             
            }
        } catch (SQLException ex) {
            Logger.getLogger(Login.class.getName()).log(Level.SEVERE, null, ex);
        }
        
        
        
        
        
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        
        
        AdminRegister regs = new AdminRegister();
        regs.main(null);
        dispose();
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        Login logs = new Login();
        logs.main(null);
        dispose();
        
        
        
    }                                        

    private void SPActionPerformed(java.awt.event.ActionEvent evt) {                                   
        // TODO add your handling code here:
        if(SP.isSelected()){
            Pass.setEchoChar((char)0);
            
            
        }else{
            Pass.setEchoChar('*');
        }
    }                                  

