        AL.main(null);
        dispose();
        
        
    }                                        

    private void SPActionPerformed(java.awt.event.ActionEvent evt) {                                   
        // TODO add your handling code here:
        
        if(SP.isSelected()){
            Pass.setEchoChar((char)0);
            
            
        }else{
            Pass.setEchoChar('*');
        }
        
        
        
    }                                  

