Connection con;
Statement st;    
    
private static final String  Dbname="registeroop";
private static final String  DbURl="jdbc:mysql://localhost:3306/"+Dbname;
private static final String  DDriver="com.mysql.cj.jdbc.Driver";
private static final String  DUsername="root";
private static final String  Dpassword="";
public void Connection () throws SQLException{
   
        
        try {
            Class.forName(DDriver);
            con= DriverManager.getConnection(DbURl,DUsername,Dpassword);
        st =  con.createStatement();
        if(con!= null){
                System.out.println("Connection Successful");
            
        }
        } catch (ClassNotFoundException ex) {
            Logger.getLogger(Register.class.getName()).log(Level.SEVERE, null, ex);
        }
        
    
