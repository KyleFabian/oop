    private void RegisterActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String username,password;
        
        if("".equals(Username.getText())){
            JOptionPane.showMessageDialog(new JFrame(),"You haven't entered your username");
            
        } else if("".equals(Password.getText())){
            JOptionPane.showMessageDialog(new JFrame(),"You haven't entered your Password");
        }else{
            username = Username.getText();
            password = Password.getText();
            String query="INSERT INTO tables(accUsername,accPassword) VALUES ('"+username+"','"+password+"')";
            try {
                st.execute(query);
            } catch (SQLException ex) {
                Logger.getLogger(Register.class.getName()).log(Level.SEVERE, null, ex);
            }
             JOptionPane.showMessageDialog(new JFrame(),"Successfully registered");
             Username.setText("");
             Password.setText("");
        }
    }                                        

